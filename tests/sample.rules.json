{
	"version": "0.1",
	"rexearch_version": "0.1.3",
	"description": "Sample rules",
	"rules":
		[

			{
				"regex" : "(Bob )?Dylan",
				"repr": "Bob Dylan"
			},

			{
				"id": "sample.rule.001",
				"regex" : "(N|n)ame ?: ?([a-zA-Z ]+)",
				"target_regex_group" : 2,
				"tags": ["PERSON_NAME"]
			},

			{
				"id": "sample.rule.002",
				"regex" : "(R|r)ate ?: ?(((V|v)ery )?(G|g)((ood)|(reat)))",
				"target_regex_group" : 2,
				"repr": "Positive",
				"tags": ["RATE", "EMOTION"]
			},

			{
				"id": "sample.rule.003",
				"regex" : "(\\d{1,3}(?:[.,]\\d{3})*) ?(D|d)ollars?",
				"target_regex_group" : 0,
				"repr": "${group(1).replace(',','')}",
				"tags": ["PRICE"]
			}
		]
}